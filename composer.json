{
    "name": "actengage/nova-passporter",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "actengage/nova-wizard": "^0.9.5|@dev",
        "laravel/framework": "^8.0",
        "laravel/nova": "^3.0",
        "laravel/passport": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Actengage\\Passporter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Actengage\\Passporter\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
